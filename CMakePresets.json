{
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 26,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "base_configure",
            "displayName": "Base Configure",
            "binaryDir": "${sourceDir}/build",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_EXPORT_COMPILE_COMMANDS": {
                    "type": "BOOL",
                    "value": "TRUE"
                },
                "LUAJIT_ENABLE_GC64": {
                    "type": "BOOL",
                    "value": "TRUE"
                },
                "ENABLE_LUA_ASSERT": {
                    "type": "BOOL",
                    "value": "TRUE"
                },
                "ENABLE_LUA_APICHECK": {
                    "type": "BOOL",
                    "value": "TRUE"
                }
            }
        },
        {
            "name": "base_asan_configure",
            "displayName": "Base ASAN Configure",
            "inherits": "base_configure",
            "hidden": true,
            "cacheVariables": {
                "ENABLE_WERROR": {
                    "type": "BOOL",
                    "value": "TRUE"
                },
                "ENABLE_ASAN": {
                    "type": "BOOL",
                    "value": "TRUE"
                },
                "ENABLE_UB_SANITIZER": {
                    "type": "BOOL",
                    "value": "TRUE"
                },
                "FIBER_STACK_SIZE": "1280Kb",
                "TEST_BUILD": {
                    "type": "BOOL",
                    "value": "TRUE"
                },
                "LUAJIT_USE_SYSMALLOC": {
                    "type": "BOOL",
                    "value": "TRUE"
                }
            }
        },
        {
            "name": "gcc_base_configure",
            "displayName": "GCC Base Configure",
            "inherits": "base_configure",
            "hidden": true,
            "environment": {
                "CC": "gcc",
                "CXX": "g++"
            }
        },
        {
            "name": "gcc_base_asan_configure",
            "displayName": "GCC Base ASAN Configure",
            "inherits": "base_asan_configure",
            "hidden": true,
            "environment": {
                "CC": "gcc",
                "CXX": "g++"
            }
        },
        {
            "name": "clang_base_configure",
            "displayName": "Clang Base Configure",
            "inherits": "base_configure",
            "hidden": true,
            "environment": {
                "CC": "clang",
                "CXX": "clang++"
            }
        },
        {
            "name": "clang_base_asan_configure",
            "displayName": "Clang Base ASAN Configure",
            "hidden": true,
            "inherits": "base_asan_configure",
            "environment": {
                "CC": "clang",
                "CXX": "clang++"
            }
        },
        {
            "name": "gcc_debug_configure",
            "displayName": "GCC Debug Configure",
            "inherits": "gcc_base_configure"
        },
        {
            "name": "gcc_release_configure",
            "displayName": "GCC Release Configure",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "LUAJIT_ENABLE_GC64": {
                    "type": "BOOL",
                    "value": "FALSE"
                },
                "ENABLE_LUA_ASSERT": {
                    "type": "BOOL",
                    "value": "FALSE"
                },
                "ENABLE_LUA_APICHECK": {
                    "type": "BOOL",
                    "value": "FALSE"
                }
            },
            "inherits": "gcc_base_configure"
        },
        {
            "name": "gcc_debug_asan_configure",
            "displayName": "GCC Debug ASAN Configure",
            "inherits": "gcc_base_asan_configure"
        },
        {
            "name": "gcc_release_asan_configure",
            "displayName": "GCC Release ASAN Configure",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "ENABLE_LUA_ASSERT": {
                    "type": "BOOL",
                    "value": "FALSE"
                },
                "ENABLE_LUA_APICHECK": {
                    "type": "BOOL",
                    "value": "FALSE"
                }
            },
            "inherits": "gcc_base_asan_configure"
        },
        {
            "name": "clang_debug_configure",
            "displayName": "Clang Debug Configure",
            "inherits": "clang_base_configure"
        },
        {
            "name": "clang_release_configure",
            "displayName": "Clang Release Configure",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "LUAJIT_ENABLE_GC64": {
                    "type": "BOOL",
                    "value": "FALSE"
                },
                "ENABLE_LUA_ASSERT": {
                    "type": "BOOL",
                    "value": "FALSE"
                },
                "ENABLE_LUA_APICHECK": {
                    "type": "BOOL",
                    "value": "FALSE"
                }
            },
            "inherits": "clang_base_configure"
        },
        {
            "name": "clang_debug_asan_configure",
            "displayName": "Clang Debug ASAN Configure",
            "inherits": "clang_base_asan_configure"
        },
        {
            "name": "clang_release_asan_configure",
            "displayName": "Clang Release ASAN Configure",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "ENABLE_LUA_ASSERT": {
                    "type": "BOOL",
                    "value": "FALSE"
                },
                "ENABLE_LUA_APICHECK": {
                    "type": "BOOL",
                    "value": "FALSE"
                }
            },
            "inherits": "clang_base_asan_configure"
        }
    ],
    "buildPresets": [
        {
            "name": "gcc_debug_build",
            "configurePreset": "gcc_debug_configure",
            "displayName": "GCC Debug Build",
            "description": "GCC Debug Build using the output of the corresponding cmake configure step"
        },
        {
            "name": "gcc_release_build",
            "configurePreset": "gcc_release_configure",
            "displayName": "GCC Release Build",
            "description": "GCC Release Build using the output of the corresponding cmake configure step"
        },
        {
            "name": "gcc_debug_asan_build",
            "configurePreset": "gcc_debug_asan_configure",
            "displayName": "GCC Debug ASAN Build",
            "description": "GCC Debug ASAN Build using the output of the corresponding cmake configure step"
        },
        {
            "name": "gcc_release_asan_build",
            "configurePreset": "gcc_release_asan_configure",
            "displayName": "GCC Release ASAN Build",
            "description": "GCC Release ASAN Build using the output of the corresponding cmake configure step"
        },
        {
            "name": "clang_debug_build",
            "configurePreset": "clang_debug_configure",
            "displayName": "Clang Debug Build",
            "description": "Clang Debug Build using the output of the corresponding cmake configure step"
        },
        {
            "name": "clang_release_build",
            "configurePreset": "clang_release_configure",
            "displayName": "Clang Release Build",
            "description": "Clang Release Build using the output of the corresponding cmake configure step"
        },
        {
            "name": "clang_debug_asan_build",
            "configurePreset": "clang_debug_asan_configure",
            "displayName": "Clang Debug ASAN Build",
            "description": "Clang Debug ASAN Build using the output of the corresponding cmake configure step"
        },
        {
            "name": "clang_release_asan_build",
            "configurePreset": "clang_release_asan_configure",
            "displayName": "Clang Release ASAN_Build",
            "description": "Clang Release ASAN Build using the output of the corresponding cmake configure step"
        }
    ],
    "testPresets": [
        {
            "name": "base",
            "configurePreset": "base_configure",
            "environment": {
                "TEST_DATA_DIR": "${sourceDir}/build"
            },
            "output": {
                "outputOnFailure": true,
                "verbosity": "verbose"
            },
            "execution": {
                "noTestsAction": "ignore",
                "scheduleRandom": true,
                "stopOnFailure": false,
                "timeout": 60
            },
            "hidden": true
        },
        {
            "name": "gcc_debug_test",
            "displayName": "GCC Debug Test",
            "inherits": "base",
            "configurePreset": "gcc_debug_configure",
            "configuration": "Debug"
        },
        {
            "name": "gcc_release_test",
            "displayName": "GCC Release Test",
            "inherits": "base",
            "configurePreset": "gcc_release_configure",
            "configuration": "RelWithDebInfo"
        },
        {
            "name": "gcc_debug_asan_test",
            "displayName": "GCC Debug ASAN Test",
            "inherits": "base",
            "configurePreset": "gcc_debug_asan_configure",
            "configuration": "Debug"
        },
        {
            "name": "gcc_release_asan_test",
            "displayName": "GCC Release ASAN Test",
            "inherits": "base",
            "configurePreset": "gcc_release_asan_configure",
            "configuration": "RelWithDebInfo"
        },
        {
            "name": "clang_debug_test",
            "displayName": "Clang Debug Test",
            "inherits": "base",
            "configurePreset": "clang_debug_configure",
            "configuration": "Debug"
        },
        {
            "name": "clang_release_test",
            "displayName": "Clang Release Test",
            "inherits": "base",
            "configurePreset": "clang_release_configure",
            "configuration": "RelWithDebInfo"
        },
        {
            "name": "clang_debug_asan_test",
            "displayName": "Clang Debug ASAN Test",
            "inherits": "base",
            "configurePreset": "clang_debug_asan_configure",
            "configuration": "Debug"
        },
        {
            "name": "clang_release_asan_test",
            "displayName": "Clang Release ASAN Test",
            "inherits": "base",
            "configurePreset": "clang_release_asan_configure",
            "configuration": "RelWithDebInfo"
        }
    ],
    "workflowPresets": [
        {
            "name": "gcc_debug_workflow",
            "displayName": "GCC Debug Workflow",
            "steps": [
                {
                    "type": "configure",
                    "name": "gcc_debug_configure"
                },
                {
                    "type": "build",
                    "name": "gcc_debug_build"
                },
                {
                    "type": "test",
                    "name": "gcc_debug_test"
                }
            ]
        },
        {
            "name": "gcc_release_workflow",
            "displayName": "GCC Release Workflow",
            "steps": [
                {
                    "type": "configure",
                    "name": "gcc_release_configure"
                },
                {
                    "type": "build",
                    "name": "gcc_release_build"
                },
                {
                    "type": "test",
                    "name": "gcc_release_test"
                }
            ]
        },
        {
            "name": "gcc_debug_asan_workflow",
            "displayName": "GCC Debug ASAN Workflow",
            "steps": [
                {
                    "type": "configure",
                    "name": "gcc_debug_asan_configure"
                },
                {
                    "type": "build",
                    "name": "gcc_debug_asan_build"
                },
                {
                    "type": "test",
                    "name": "gcc_debug_asan_test"
                }
            ]
        },
        {
            "name": "gcc_release_asan_workflow",
            "displayName": "GCC Release ASAN Workflow",
            "steps": [
                {
                    "type": "configure",
                    "name": "gcc_release_asan_configure"
                },
                {
                    "type": "build",
                    "name": "gcc_release_asan_build"
                },
                {
                    "type": "test",
                    "name": "gcc_release_asan_test"
                }
            ]
        },
        {
            "name": "clang_debug_workflow",
            "displayName": "Clang Debug Workflow",
            "steps": [
                {
                    "type": "configure",
                    "name": "clang_debug_configure"
                },
                {
                    "type": "build",
                    "name": "clang_debug_build"
                },
                {
                    "type": "test",
                    "name": "clang_debug_test"
                }
            ]
        },
        {
            "name": "clang_release_workflow",
            "displayName": "Clang Release Workflow",
            "steps": [
                {
                    "type": "configure",
                    "name": "clang_release_configure"
                },
                {
                    "type": "build",
                    "name": "clang_release_build"
                },
                {
                    "type": "test",
                    "name": "clang_release_test"
                }
            ]
        },
        {
            "name": "clang_ebug_asan_workflow",
            "displayName": "clang Debug ASAN Workflow",
            "steps": [
                {
                    "type": "configure",
                    "name": "clang_debug_asan_configure"
                },
                {
                    "type": "build",
                    "name": "clang_debug_asan_build"
                },
                {
                    "type": "test",
                    "name": "clang_debug_asan_test"
                }
            ]
        },
        {
            "name": "clang_release_asan_workflow",
            "displayName": "Clang Release ASAN Workflow",
            "steps": [
                {
                    "type": "configure",
                    "name": "clang_release_asan_configure"
                },
                {
                    "type": "build",
                    "name": "clang_release_asan_build"
                },
                 {
                    "type": "test",
                    "name": "clang_release_asan_test"
                }
            ]
        }
    ]
}
